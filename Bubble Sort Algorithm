def bubble_sort(a):
    while True:
        swapped = False
        for i in range(len(a)-1):
            if a[i] > a[i+1]:
                a[i], a[i+1] = a[i+1], a[i]
                swapped = True 
        if not swapped: break

data = [35, 12, 22, 54, 41, 78, 95]
print("Given data:", data)

bubble_sort(data)
print("Output data:", data)
